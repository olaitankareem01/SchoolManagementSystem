<h3 class="text-center mt-5 mb-2">Audit Logs</h3>
<div class="row">
    <div class="col-4 mb-3">
        <input class="form-control fa fa-search" id="myInput" type="text" placeholder="Search..">
    </div>
</div>
<table class="table table-responsive table-striped table-bordered table-dark">
    <thead class="thead-light">
        <th>ID</th>
        <th>ACTION</th>
        <th>DETAIL</th>
        <th>PERFORMED BY</th>
        <th>DATE PERFORMED</th>
        <th colspan='1'>ACTION</th>


    </thead>
    <tbody id="tablerow">
        {{#each data}}
        <tr>
            <td>{{ID}}</td>
            <td>{{action}}</td>
            <td>{{detail}}</td>
            <td>{{performedby}}</td>
            <td>{{date_performed}}</td>
        </tr>
        {{/each}}
    </tbody>
</table>