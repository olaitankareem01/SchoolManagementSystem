<h3 class="text-center mt-5 mb-2">Audit Logs</h3>
<div class="row">
    <div class="col-4 mb-3">
        <input class="form-control fa fa-search" id="myInput" type="text" placeholder="Search..">
    </div>
</div>
<table class="table table-responsive table-striped table-bordered table-dark">
    <thead class="thead-light">
        <th>ID</th>
        <th>NAME</th>
        <th>EMAIL</th>
        <th>MESSAGE</th>
        <th>DATE CONTACTED</th>
        <th colspan='1'>ACTION</th>


    </thead>
    <tbody id="tablerow">
        {{#each data}}
        <tr>
            <td>{{ID}}</td>
            <td>{{name}}</td>
            <td>{{email}}</td>
            <td>{{message}}</td>
            <td>{{date_contacted}}</td>
            <td><a class="btn btn-sm btn-danger" href="/contact/delete/{{ID}}">DELETE</a></td>
        </tr>
        {{/each}}
    </tbody>
</table>