<div class="container mt-5">
    <h3>List OF Users</h3>
    <div class="row">
        <div class="col-4 mb-3">
            <input class="form-control fa fa-search" id="myInput" type="text" placeholder="Search..">
        </div>
    </div>
    <table class="table  table-striped table-bordered table-dark table-responsive-sm table-hover mt-3" id="userstable">
        <thead>
            <th>ID</th>
            <th>FIRST NAME</th>
            <th>LAST NAME</th>
            <th>EMAIL</th>
            <th>PHONE NO</th>
            <th colspan="3">ACTION</th>
        </thead>

        <tbody id="tablerow">
            {{#each data}}
            <tr>

                <td>{{ID}}</td>
                <td>{{firstname}}</td>
                <td>{{lastname}}</td>
                <td>{{email}}</td>
                <td> {{phoneno}}</td>
                <td> <a class="btn btn-sm btn-success" href="/users/assignrole/{{ID}}">ASSIGN ROLE</a></td>
                <td> <a class="btn btn-sm btn-warning" href="/users/edit/{{ID}}">EDIT</a></td>
                <td> <a class="btn btn-sm btn-danger" href="/users/delete/{{ID}}">DELETE</a></td>
            </tr>
            {{/each}}
        </tbody>

    </table>
</div>
